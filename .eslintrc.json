{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "plugins": [
        "meteor",
        "react"
    ],
    "extends": [
        "airbnb",
        "plugin:meteor/recommended"
    ],
    "rules": {
      "comma-dangle": 0,
      "no-alert": 0,
      "no-underscore-dangle": 0,
      "camelcase": 0,
      "no-return-assign": 0,
      "prefer-template": 0,
      "meteor/eventmap-params": [
          2,
          {
              "templateInstanceParamName": "instance"
          }
      ],
      "import/no-unresolved": [
          2,
          {
              "ignore": [
                  "^meteor/"
              ]
          }
      ]
    }
}
